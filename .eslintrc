{
    "root": true,
    "parserOptions": {
       "parser": "babel-eslint"
    },
    "env":{
     "browser":true,
     "node":true
    },
    "extends": [
       "eslint:recommended",
       "plugin:vue/essential"
     ],
    "plugins": ["@vue"],
    "rules": {
       "no-console": "off",
       "no-constant-condition":"off",
       "no-global-assign":"off",
       "no-empty":"off",
       "no-useless-escape":"off",
       "no-redeclare":"off",
       "no-undef":"off",
       "no-mixed-spaces-and-tabs":"off",
       "no-unused-vars":"off"
    }
 }